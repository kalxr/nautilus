./scripts/pass_build ; make clean ; make -j ; make timing ; make isoimage
cp nautilus.bin $1/nautilus200.bin ; cp nautilus.iso $1/nautilus200.iso
sed -i "s/NAUT_CONFIG_COMPILER_TIMING_PERIOD_CYCLES=200/NAUT_CONFIG_COMPILER_TIMING_PERIOD_CYCLES=400/g" .config
sed -i "s/GRAN\ 200/GRAN\ 400/g" src/llvm/compiler-timing/include/Configurations.hpp 

